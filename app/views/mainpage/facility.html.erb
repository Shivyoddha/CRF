<!--JS code for the switcher-->
<script>
    let manuSec = document.getElementById('manufacturing');
    let charSec = document.getElementById('characterization');
    let processSec = document.getElementById('processing');
    let process = document.getElementById('process');
    let char = document.getElementById('char');
    let manu = document.getElementById('manu');
    let proHead = document.getElementById('process-h');
    let charHead = document.getElementById('char-h');
    let manuHead = document.getElementById('manu-h');

    manu.addEventListener('click', ()=> {
        // make the manufacturing section visible
        manuSec.classList.remove('vis');

        //rest of the sections hidden
        charSec.classList.add('vis');
        processSec.classList.add('vis');

        //add grid property accordingly
        manuSec.classList.add('grid');
        processSec.classList.remove('grid');
        charSec.classList.remove('grid');

        //make the manufacturing heading visible
        manuHead.classList.remove('vis');
        //rest of the headings hidden
        proHead.classList.add('vis');
        charHead.classList.add('vis');

        //add background color to button of manufacturing
        manu.classList.add('backcolor');

        //remove background color to button of manufacturing
        char.classList.remove('backcolor');
        process.classList.remove('backcolor');

    })
    char.addEventListener('click', ()=> {
        // make the char section visible
        charSec.classList.remove('vis');
        //rest of the sections hidden
        processSec.classList.add('vis');
        manuSec.classList.add('vis');

        //add grid property accordingly
        charSec.classList.add('grid');
        manuSec.classList.remove('grid');
        processSec.classList.remove('grid');

        //make the char heading visible
        charHead.classList.remove('vis');
        //rest of the headings hidden
        proHead.classList.add('vis');
        manuHead.classList.add('vis');

        //add background color to button of char
        char.classList.add('backcolor');
        //remove background color to button of manufacturing
        process.classList.remove('backcolor');
        manu.classList.remove('backcolor');
    })
    process.addEventListener('click', ()=> {
        // make the process section visible
        processSec.classList.remove('vis');
        //rest of the sections hidden
        charSec.classList.add('vis');
        manuSec.classList.add('vis');

        //add grid property accordingly
        manuSec.classList.remove('grid');
        processSec.classList.add('grid');
        charSec.classList.remove('grid');

        //make the char heading visible
        proHead.classList.remove('vis');
        //rest of the headings hidden
        charHead.classList.add('vis');
        manuHead.classList.add('vis');

        //add background color to button of process
        process.classList.add('backcolor');
        //remove background color to button of manufacturing
        char.classList.remove('backcolor');
        manu.classList.remove('backcolor');
    })
</script>
<!--switcher JS code ends here-->
