    <script>
        let user_img_1 = document.getElementById("user_img_1");
        let user_img_2 = document.getElementById("user_img_2");
        let profile_options = document.getElementById("profile_options");
        let count = 0;
        user_img_1.addEventListener("click", () => {
            if (count % 2 === 0) {
                profile_options.style.display = "none";
            }
            else {
                profile_options.style.display = "flex";

            }

            count++
        })
        user_img_2.addEventListener("click", () => {
            if (count % 2 === 0) {
                profile_options.style.display = "none";
            }
            else {
                profile_options.style.display = "flex";

            }

            count++
        })

    </script>
